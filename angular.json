{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-smooth-carousel": {
      "projectType": "library",
      "root": "projects/ng-smooth-carousel",
      "sourceRoot": "projects/ng-smooth-carousel/src",
      "prefix": "nsc",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-smooth-carousel/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-smooth-carousel/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-smooth-carousel/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-smooth-carousel/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "angular14-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/angular14-demo",
      "sourceRoot": "projects/angular14-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular14-demo",
            "index": "projects/angular14-demo/src/index.html",
            "main": "projects/angular14-demo/src/main.ts",
            "polyfills": "projects/angular14-demo/src/polyfills.ts",
            "tsConfig": "projects/angular14-demo/tsconfig.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular14-demo/src/favicon.ico",
              "projects/angular14-demo/src/assets"
            ],
            "styles": ["projects/angular14-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/angular14-demo/src/environments/environment.ts",
                  "with": "projects/angular14-demo/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular14-demo:build:production"
            },
            "development": {
              "browserTarget": "angular14-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular14-demo/src/test.ts",
            "polyfills": "projects/angular14-demo/src/polyfills.ts",
            "tsConfig": "projects/angular14-demo/tsconfig.spec.json",
            "karmaConfig": "projects/angular14-demo/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/angular14-demo/src/favicon.ico",
              "projects/angular14-demo/src/assets"
            ],
            "styles": ["projects/angular14-demo/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
